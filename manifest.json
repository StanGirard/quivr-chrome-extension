{
    "manifest_version": 3,
    "name": "Quivr",
    "version": "0.0.1",
    "permissions": ["activeTab", "storage","notifications"],
    "action": {
        "default_icon": {
          "48": "128.png"
        }
    },
    "background": {
      "service_worker": "background.js"
    },
    "commands": {
      "send_url": {
        "suggested_key": {
          "default": "Ctrl+Shift+Y",
          "mac": "Command+Shift+Y"
        },
        "description": "Quivr"
      }
    },
    "options_page": "options.html"
  }
  